<script>
var zS = z$ ? true : false;
var i_ = <%我的参%>;
function max__() {
	return opener.img_max_;
}

function w_h__(w, h) {
	img_.style.maxWidth = null;
	img_.style.width = w;
	img_.style.height = h;
}

function z__() {
	var out1 = 0, out2 = 0;
	var e;
	for(;;) {
		if(i_ < 1) {
			i_ = 1;
			out1++;
		}
		if(i_ > max__()) {
			i_ = max__();
			out2++;
		}
		if(out1 > 2 || out2 > 2)
			break;
		e = o_e__("i" + i_);
		if(e)
			break;
		old_key__(out1 > 1 || out2 > 1 ? "Z" : "z");
	}
	if(e) {
		src_ = e.value;
		img1_.src = src_;
	}
	img_1_.style.marginTop = 0;
	r_ = 0; s_ = sx_ = sy_ = fx_ = 1;
	auto_i_ = 0;
	transform__();
	title__();
	title_i__();
	title_r__();
	title_s__();
	title_old_key__();
}
function z2__() {
	img_.src = img1_.src;
	w_h__(null, null);
	var h1 = img1_.height,
		h2 = document.body.clientHeight,
		w1 = img1_.width,
		w2 = document.body.clientWidth;
	if(h1 > h2 || w1 > w2) {
		if(h1 / h2 > w1 / w2) {
			img_.style.maxWidth = "auto";
			img_.style.height = "100%";
		} else {
			img_.style.maxWidth = "100%";
			img_.style.height = "auto";
		}
	}
}
function title__() {htm__("title", src_);}
function title_i__() {htm__("title_i", i_ + ".." + max__());}
function title_old_key__() {htm__("title_old_key", old_key_);}
function title_r__() {htm__("title_r", r_);}
function title_s__() {htm__("title_s", s_.toFixed(2));}
function resize__() {
	z2__();
	var i = document.body.clientHeight / 44, min = 7, max = 13;
	if(i < min)
		i = min;
	else if(i > max)
		i = max;
	e__("bar").style.fontSize = i + "px";
}
var r_, s_, sx_, sy_, fx_;
function transform__() {
	img_.style.webkitTransform = img_.style.transform = 
		"rotate(" + r_ + "deg)scale(" + (fx_ * sx_) + "," + sy_ + ")";
	/*auto_pause__((r_ % 360) != 0 || fx_ != 1 || sx_ != 1 || sy_ != 1);*/
}
/*function s__(s) {sx_ = sy_ = s; transform__();}*/
function w_h_s__() {
	w_h__(img1_.width * s_, img1_.height * s_);
	/*auto_pause__(s_ != 0);*/
	title_s__();
}
function s1__() {s_ = 1; w_h_s__();}
function s__() {s_ += 0.01; w_h_s__();}
function ss__() {s_ -= 0.01; w_h_s__();}
function r__() {r_ += 15; transform__(); title_r__();}
function rr__() {r_ -= 15; transform__(); title_r__();}
function f__() {fx_ = fx_ == 1 ? -1 : 1; transform__();}
function first__() {i_ = 1; z__();}
function back__() {back_1__(); z__();}
function next__() {next_1__(); z__();}
function end__() {end_1__(); z__();}
function rand__() {rand_1__(); z__();}
function back_1__() {i_--;}
function next_1__() {i_++;}
function end_1__() {i_ = max__();}
function rand_1__() {i_ = Math.floor(Math.random() * max__()) + 1;}
function reload__() {z__();z2__();}
var auto_, auto_pause_;<%赋予等以等。%>
function auto_pause__(b) {
	if(!auto_)
		return;
	auto_pause_ = b;
	htm__("auto", b ? "停" : "<%‘等’%>");
}
function auto__(ctl) {
	if(ctl && auto_)
		return;
	if(!auto_) {
		auto_ = setInterval(auto_2__, 1000);
		auto_pause_ = false;
	} else {
		clearInterval(auto_);
		auto_ = null;
	}
	htm__("auto", auto_ ? "<%‘等’%>" : "");
	htm__("auto2", auto_ ? "y" : "");
}
var auto_i_;
function auto_2__() {
	if(auto_pause_)
		return;
	var max = 60 * 4;
	if(++auto_i_ >= max)
		auto_i_ = 0;
	htm__("auto", max - auto_i_);
	if(auto_i_ != 0)
		return;
	old_key__();
}
var old_key_ = "N";
function old_key__(nor) {
	switch(nor) {
	case true:
		switch(old_key_) {
		case "B":
			back_1__();
			break;
		default:
			next_1__();
			break;
		}
		z__();
		break;
	default:
		switch(old_key_) {
		case "E":
			end_1__();
			break;
		case "R":
			rand_1__();
			break;
		case "B":
			back_1__();
			break;
		default:
			next_1__();
			break;
		}
		z__();
		break;
	case "z":
		switch(old_key_) {
		case "B":
			back_1__();
			break;
		default:
			next_1__();
			break;
		}
		break;
	case "Z":
		switch(old_key_) {
		case "B":
			next_1__();
			break;
		default:
			back_1__();
			break;
		}
		break;
	}
}
function set_change__() {
	auto_pause__(true);
	opener.set_change__(function(s, change) {
		opener.set_change__(null);
		focus__(window, s);
		auto_pause__(false);
		if(change)
			old_key__(true);
	});
	opener.sel_all__(false);
	o_e__("i" + i_).checked = true;
}
function mv__() {set_change__();opener.mv__();}
function rm__() {set_change__();opener.rm__();}
function quit__() {window.close();}
function fs__() {
	var b = document.fullScreen || document.mozFullScreen || document.webkitIsFullScreen;
	if(b) {
		var de = document;
		if (de.exitFullscreen) {
			de.exitFullscreen();
		} else if (de.mozCancelFullScreen) {
			de.mozCancelFullScreen();
		} else if (de.webkitCancelFullScreen) {
			de.webkitCancelFullScreen();
		}
	} else {
		var de = document.documentElement;
		if (de.requestFullscreen) {
			de.requestFullscreen();
		} else if (de.mozRequestFullScreen) {
			de.mozRequestFullScreen();
		} else if (de.webkitRequestFullScreen) {
			de.webkitRequestFullScreen();
		}
	}
}
function up__(up) {
	var s = img_1_.style, i = up ? -1 : 1;
	var i2 = parseInt(s.marginTop);
	if(!i2)
		i2 = 0;
	s.marginTop = i2 + i * 10;
}
function kao__() {
	var s = e__("bar").style;
	if(s.left == "0px") {
		s.right = "0px";
		s.left = null;
		htm__("kao", "左");
	} else {
		s.left = "0px";
		s.right = null;
		htm__("kao", "右");
	}
}
function bar_hide__(ctl) {
	var bar = e__("bar").style, bar2 = e__("bar2").style;
	var i = 0;
	if(bar.display != "none")
		i = 1;
	else if(bar2.display != "none")
		i = 2;
	if(ctl) {
		switch(i) {
		case 2:
			return;
		case 0:
			bar2.display = "block";
			break;
		}
		i = 1;
	}
	switch(i) {
	case 1:
		bar.display = "none";
		break;
	case 2:
		bar2.display = "none";
		break;
	case 0:
		bar.display = bar2.display = "block";
		break;
	}
	if(zS)
		switch(i) {
		case 0: case 1:
			z$("使主窗、无标签、" + i + "、无边框、" + i);
			break;
		}
}
window.document.onkeydown = function(evt) {
	var kc = evt.keyCode;
	var key = String.fromCharCode(kc);
	switch(key) {
	case "B":
	case "N":
	case "R":
	case "E":
		old_key_ = key;
		title_old_key__();
		break;
	case " ": /*case "\r":*/
		key = old_key_;
		break;
	}
	switch(kc) {
	case 113:
		e__("auto_onblur").checked = false;
		auto__(true);
		bar_hide__(true);
		if(old_key_ != "R") {
			old_key_ = "R";
			rand__();
		}
		return;
	}
	switch(key) {
	case "F":fs__();return;
	case "B":back__();return;
	case "N":next__();break;
	case "E":end__();return;
	case "R":rand__();return;
	case "1":s1__();return;
	case "A":s__();return;
	case "S":ss__();return;
	case "X":r__();return;
	case "Z":rr__();return;
	case "Q":quit__();return;
	case "H":bar_hide__();return;
	case "O":up__(true);return;
	case "P":up__(false);return;
	case "D":rm__();return;
	case "M":mv__();return;
	case "U":auto__();return;
	case "T":e__("auto_onblur").click();return;
	}
	if(zS)
		switch(kc) {
		case 116:location.reload();return;
		case 122:fs__();return;
		}
};
</script>
