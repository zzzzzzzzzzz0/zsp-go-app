<script>
var i_ = <%我的参%>;
function max__() {
	return opener.img_max_;
}

function w_h__(w, h) {
	img_.style.maxWidth = null;
	img_.style.width = w;
	img_.style.height = h;
}

function z__() {
	if(i_ < 1)
		i_ = 1;
	else if(i_ > max__())
		i_ = max__();
	var e = o_e__("i" + i_);
	if(!e) {
		old_key__();
		return;
	}
	src_ = e.value;
	img1_.src = src_;
	img_1_.style.marginTop = 0;
	r_ = 0; s_ = sx_ = sy_ = fx_ = 1;
	auto_i_ = 0;
	transform__();
	title__();
	title_i__();
	title_r__();
	title_old_key__();
}
function z2__() {
	img_.src = img1_.src;
	w_h__(null, null);
	var h1 = img1_.height,
		h2 = document.body.clientHeight,
		w1 = img1_.width,
		w2 = document.body.clientWidth;
	if(h1 > h2 || w1 > w2) {
		if(h1 / h2 > w1 / w2) {
			img_.style.maxWidth = "auto";
			img_.style.height = "100%";
		} else {
			img_.style.maxWidth = "100%";
			img_.style.height = "auto";
		}
	}
}
function title__() {
	htm__("title", src_);
}
function title_i__() {
	htm__("title_i", i_ + ".." + max__());
}
function title_old_key__() {
	htm__("title_old_key", old_key_);
}
function title_r__() {
	htm__("title_r", r_);
}
function resize__() {
	z2__();
	var i = document.body.clientHeight / 44, min = 7, max = 13;
	if(i < min)
		i = min;
	else if(i > max)
		i = max;
	e__("bar").style.fontSize = i + "px";
}
var r_, s_, sx_, sy_, fx_;
function transform__() {
	img_.style.webkitTransform = img_.style.transform = 
		"rotate(" + r_ + "deg)scale(" + (fx_ * sx_) + "," + sy_ + ")";
	auto_pause__((r_ % 360) != 0 || fx_ != 1 || sx_ != 1 || sy_ != 1);
}
/*function s__(s) {
	sx_ = sy_ = s; transform__();
}*/
function w_h_s__() {
	w_h__(img1_.width * s_, img1_.height * s_);
	auto_pause__(s_ != 0);
}
function s1__() {
	s_ = 1; w_h_s__();
}
function s__() {
	s_ += 0.01; w_h_s__();
}
function ss__() {
	s_ -= 0.01; w_h_s__();
}
function r__() {
	r_ += 15; transform__(); title_r__();
}
function rr__() {
	r_ -= 15; transform__(); title_r__();
}
function f__() {
	fx_ = fx_ == 1 ? -1 : 1; transform__();
}
function first__() {
	i_ = 1; z__();
}
function back__() {
	i_--; z__();
}
function next__() {
	i_++; z__();
}
function end__() {
	i_ = max__(); z__();
}
function rand__() {
	i_ = Math.floor(Math.random() * max__()) + 1; z__();
}
function reload__() {
	z__();
	z2__();
}
var auto_, auto_pause_;
function auto_pause__(b) {
	if(!auto_)
		return;
	auto_pause_ = b;
	htm__("auto", b ? "?" : "*");
}
function auto__() {
	if(!auto_) {
		auto_ = setInterval(auto_2__, 1000);
		auto_i_ = 0;
		auto_pause_ = false;
	} else {
		clearInterval(auto_);
		auto_ = null;
	}
	htm__("auto", auto_ ? "*" : "");
	htm__("auto2", auto_ ? "y" : "");
}
var auto_i_;
function auto_2__() {
	if(auto_pause_)
		return;
	var max = 30;
	if(++auto_i_ >= max)
		auto_i_ = 0;
	htm__("auto", max - auto_i_);
	if(auto_i_ != 0)
		return;
	switch(old_key_) {
	case "B":
		if(i_ == 1)
			i_ = max__() + 1;
		break;
	case "N":
		if(i_ == max__())
			i_ = 0;
		break;
	}
	old_key__();
}
var old_key_ = "N";
function old_key__(nor) {
	if(!nor) {
		switch(old_key_) {
		case "R":
			rand__();
			return;
		case "E":
			end__();
			return;
		}
	}
	switch(old_key_) {
	case "B":
		back__();
		return;
	}
	next__();
}
function set_change__(f) {
	auto_pause__(true);
	opener.set_change__(function(s) {
		opener.set_change__(null);
		focus__(window, s);
		auto_pause__(false);
		old_key__(true);
	});
	opener.sel_all__(false);
	o_e__("i" + i_).checked = true;
}
function mv__() {
	set_change__();
	opener.mv__();
}
function rm__() {
	set_change__();
	opener.rm__();
}
function quit__() {
	window.close();
}
function fs__() {
	if(document.fullScreen || document.mozFullScreen || document.webkitIsFullScreen) {
		var de = document;
		if (de.exitFullscreen) {
			de.exitFullscreen();
		} else if (de.mozCancelFullScreen) {
			de.mozCancelFullScreen();
		} else if (de.webkitCancelFullScreen) {
			de.webkitCancelFullScreen();
		}
	} else {
		var de = document.documentElement;
		if (de.requestFullscreen) {
			de.requestFullscreen();
		} else if (de.mozRequestFullScreen) {
			de.mozRequestFullScreen();
		} else if (de.webkitRequestFullScreen) {
			de.webkitRequestFullScreen();
		}
	}
}
function up__(up) {
	var s = img_1_.style, i = up ? -1 : 1;
	var i2 = parseInt(s.marginTop);
	if(!i2)
		i2 = 0;
	s.marginTop = i2 + i * 10;
}
function kao__() {
	var s = e__("bar").style;
	if(s.left == "0px") {
		s.right = "0px";
		s.left = null;
		htm__("kao", "左");
	} else {
		s.left = "0px";
		s.right = null;
		htm__("kao", "右");
	}
}
function bar_hide__() {
	var bar = e__("bar").style, bar2 = e__("bar2").style;
	if(bar.display != "none") {
		bar.display = "none";
		if(z$) z$("使主窗、无标签");
		return;
	}
	if(bar2.display != "none") {
		bar2.display = "none";
		return;
	}
	bar.display = bar2.display = "block";
	if(z$) z$("使主窗、无标签、0");
}
window.document.onkeydown = function(evt) {
	var key = String.fromCharCode(evt.keyCode);
	switch(key) {
	case "B":
	case "N":
	case "R":
	case "E":
		old_key_ = key;
		title_old_key__();
		break;
	case " ":
		key = old_key_;
		break;
	}
	switch(key) {
	case "F":
		fs__();
		break;
	case "B":
		back__();
		break;
	case "N":
		next__();
		break;
	case "E":
		end__();
		break;
	case "R":
		rand__();
		break;
	case "1":
		s1__();
		break;
	case "A":
		s__();
		break;
	case "S":
		ss__();
		break;
	case "X":
		r__();
		break;
	case "Z":
		rr__();
		break;
	case "Q":
		quit__();
		break;
	case "H":
		bar_hide__();
		break;
	case "O":
		up__(true);
		break;
	case "P":
		up__(false);
		break;
	case "D":
		rm__();
		break;
	case "M":
		mv__();
		break;
	case "U":
		auto__();
		break;
	/*default:
		alert(key);
		break;*/
	}
};
</script>
